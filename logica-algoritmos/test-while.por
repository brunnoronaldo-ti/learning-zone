Algoritmo "idades"

Var
 idade, resultado : inteiro



Inicio
   escreval("DIGITE UMA IDADE: ")
   leia(idade)
   
   enquanto idade >  = 0 faca
      escreval("DIGITE OUTRA: ")
      leia(idade)
   fimenquanto

   resultado <- idade
   escreval("A SOMA DE TODAS AS IDADES:", resultado)
Fimalgoritmo
